{
  "name": "@commercetools-test-data/core",
  "version": "1.1.0",
  "description": "This package provides the core functions to define the data models",
  "bugs": "https://github.com/commercetools/test-data/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/test-data.git",
    "directory": "core"
  },
  "keywords": [
    "javascript",
    "test-data"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/core-index.cjs.js",
  "module": "./dist/core-index.es.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:bundles && yarn build:typings",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../rollup.config.js -i ./src/index.ts --dir dist",
    "build:bundles:watch": "yarn build:bundles -w",
    "build:typings": "cross-env tsc -p tsconfig.declarations.json --emitDeclarationOnly --declarationDir dist/typings"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "@babel/runtime-corejs3": "7.11.2",
    "@jackfranklin/test-data-bot": "1.3.0"
  }
}
